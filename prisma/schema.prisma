generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model permissions {
  id                   Int                    @id @default(autoincrement())
  name                 String                 @unique
  grup                 String?                @db.VarChar
  role_has_permissions role_has_permissions[]
}

model role_has_permissions {
  role_id       Int
  permission_id Int
  permissions   permissions @relation(fields: [permission_id], references: [id], onDelete: Cascade, onUpdate: Restrict)
  roles         roles       @relation(fields: [role_id], references: [id], onDelete: Cascade, onUpdate: Restrict)

  @@id([role_id, permission_id])
  @@index([permission_id])
}

model roles {
  id                   Int                    @id @default(autoincrement())
  name                 String                 @unique
  role_has_permissions role_has_permissions[]
  user_roles           user_roles[]
}

model user_roles {
  user_id Int
  role_id Int
  roles   roles @relation(fields: [role_id], references: [id], onDelete: Cascade, onUpdate: Restrict)
  users   users @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: Restrict)

  @@id([user_id, role_id])
  @@index([role_id])
}

model users {
  id         Int          @id @default(autoincrement())
  nama       String
  email      String       @unique
  password   String
  banner     banner[]
  keunggulan keunggulan[]
  portofolio portofolio[]
  user_roles user_roles[]
}

model portofolio {
  id          Int       @id(map: "portfolios_pkey") @default(autoincrement())
  name        String    @db.VarChar(255)
  slug        String    @unique(map: "portfolios_slug_key") @db.VarChar(255)
  image       String?   @db.VarChar(255)
  description String?
  created_by  Int
  created_at  DateTime? @default(now()) @db.Timestamp(6)
  updated_at  DateTime? @default(now()) @db.Timestamp(6)
  users       users     @relation(fields: [created_by], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "fk_portofolio_user")
}

model kontak {
  id         Int       @id @default(autoincrement())
  platform   String    @db.VarChar(50)
  url        String
  icon       String    @db.VarChar(50)
  order      Int       @default(0)
  is_active  Boolean   @default(true)
  created_at DateTime? @default(now()) @db.Timestamp(6)
  updated_at DateTime? @default(now()) @db.Timestamp(6)
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model tim {
  id         Int       @id @default(autoincrement())
  nama       String    @db.VarChar(100)
  posisi     String    @db.VarChar(100)
  created_at DateTime? @default(now()) @db.Timestamp(6)
  updated_at DateTime? @default(now()) @db.Timestamp(6)
  image      String?   @db.VarChar
}

model chat {
  id          Int       @id @default(autoincrement())
  title       String    @db.VarChar(255)
  description String
  discount    Int
  created_at  DateTime? @default(now()) @db.Timestamp(6)
  updated_at  DateTime? @default(now()) @updatedAt @db.Timestamp(6)
}

model faq {
  id         Int       @id @default(autoincrement())
  question   String    @db.VarChar(255)
  answer     String
  isActive   Boolean?  @default(true)
  sortOrder  Int?      @default(0)
  created_at DateTime? @default(now()) @db.Timestamp(6)
  updated_at DateTime? @default(now()) @updatedAt @db.Timestamp(6)
}

model paket {
  id         Int           @id @default(autoincrement())
  name       String        @db.VarChar(255)
  harga      String        @db.VarChar(50)
  created_at DateTime?     @default(now()) @db.Timestamp(6)
  updated_at DateTime?     @default(now()) @updatedAt @db.Timestamp(6)
  fitur      paket_fitur[]
}

model paket_fitur {
  id       Int    @id @default(autoincrement())
  id_paket Int
  fitur    String @db.VarChar(255)
  paket    paket  @relation(fields: [id_paket], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_paket")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model misi {
  id         Int     @id @default(autoincrement())
  text       String
  profile_id Int
  profile    profile @relation(fields: [profile_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model profile {
  id         Int       @id @default(autoincrement())
  sejarah    String
  visi       String
  created_at DateTime? @default(now()) @db.Timestamp(6)
  updated_at DateTime? @default(now()) @db.Timestamp(6)
  misi       misi[]
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model payment_step {
  id                  Int                   @id @default(autoincrement())
  step_number         Int
  title               String?               @db.VarChar
  createdat           DateTime?             @default(now()) @db.Timestamp(6)
  updatedat           DateTime?             @default(now()) @db.Timestamp(6)
  payment_step_detail payment_step_detail[]
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model payment_step_detail {
  id              Int          @id @default(autoincrement())
  sub_title       String       @db.VarChar(255)
  description     String
  createdat       DateTime?    @default(now()) @db.Timestamp(6)
  updatedat       DateTime?    @default(now()) @db.Timestamp(6)
  payment_step_id Int
  payment_step    payment_step @relation(fields: [payment_step_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_payment_step")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model keunggulan {
  id          Int       @id @default(autoincrement())
  title       String    @db.VarChar(255)
  slug        String    @unique @db.VarChar(255)
  image       String?   @db.VarChar(255)
  description String?
  created_by  Int
  created_at  DateTime? @default(now()) @db.Timestamp(6)
  updated_at  DateTime? @default(now()) @db.Timestamp(6)
  users       users     @relation(fields: [created_by], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "fk_keunggulan_user")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model banner {
  id         Int       @id @default(autoincrement())
  image      String    @db.VarChar(255)
  is_active  Boolean   @default(true)
  created_by Int
  created_at DateTime? @default(now()) @db.Timestamp(6)
  updated_at DateTime? @default(now()) @db.Timestamp(6)
  users      users     @relation(fields: [created_by], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "fk_banner_user")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model kelebihan_kekurangan {
  id                          Int                           @id @default(autoincrement())
  judul                       String                        @db.VarChar(255)
  type                        String                        @db.VarChar(50)
  created_at                  DateTime                      @default(now()) @db.Timestamp(6)
  updated_at                  DateTime                      @default(now()) @db.Timestamp(6)
  kelebihan_kekurangan_detail kelebihan_kekurangan_detail[]
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model kelebihan_kekurangan_detail {
  id                      Int                  @id @default(autoincrement())
  kelebihan_kekurangan_id Int
  detail                  String
  created_at              DateTime             @default(now()) @db.Timestamp(6)
  updated_at              DateTime             @default(now()) @db.Timestamp(6)
  kelebihan_kekurangan    kelebihan_kekurangan @relation(fields: [kelebihan_kekurangan_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_kelebihan_kekurangan")
}
